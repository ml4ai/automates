                    "node_type": "SourceRef",
                    "row_end": [
                        "def main(x, c):\n",
                        "    a = 0\n",
                        "    while c > 0:\n",
                        "        a = a + (x * c)\n",
                        "        c = c - 1\n",
                        "    return a\n"
                    ],
                    "row_start": 1,
                    "source_file_name": "simple_while_loop.py"
                }
