# exp1.py
[
	{  # This is FN 1
		"B": [{"name": "module", "type": "Module"}],  # The FN outer Box as a Module
		"BF": [{"name": "", "type": "Expression", "contents": 2, "value": None}],  # "contents" = FN 2
		"POF": [{"box": 1, "name": "x"}]  # The PortOut for the BF
	},
	{  # This is FN 2
		"B": [{"name": "assign_x", "type": "Expression"}],  # The FN outer Box as an Expression
		"BF": [{"name": "", "type": "LiteralValue", "contents": None, "value": 2},
					 {"name": "", "type": "LiteralValue", "contents": None, "value": 3},
					 {"name": "+", "type": "PrimitiveOp", "contents": None, "value": None}],
		"PIF": [{"box": 3, "name": ""},   # The 'left' PortIn for the PrimitiveOp + (BF 3)
						{"box": 3, "name": ""}],  # The 'right' PortIn for the PrimitiveOp + (BF 3)
		"POF": [{"box": 1, "name": ""},   # The PortOut for the LiteralValue 2 (BF 1)
						{"box": 2, "name": ""},   # The PortOut for the LiteralValue 3 (BF 2)
						{"box": 3, "name": ""}],  # The PortOut for the PrimitiveOp + (BF 3)
		"WFF": [{"src": 1, "tgt": 1},  # The Wire from POF 1 of BF 1 to PIF 1 of BF 3
		        {"src": 2, "tgt": 2}],  # The Wire from POF 2 of BF 2 to PIF 2 of BF 3
		"OPO": [{"box": 1, "name": "x"}],   # The OuterPortOut, with "box" referring to index in "B"
		"WFOPO": [{"src": 3, "tgt": 1}]  # The Wire from PrimitiveOp + (Function) PortOut ("POF" 3) to the OuterPortOut ("OPO" 1)
	}
]